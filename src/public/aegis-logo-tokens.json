    "sizes": {
      "sm": {
        "logoSize": "3rem",
        "outerGlow": "3rem",
        "innerGlow": "2.5rem",
        "pixels": 48
      },
      "md": {
        "logoSize": "5rem",
        "outerGlow": "5rem",
        "innerGlow": "4rem",
        "pixels": 80
      },
      "lg": {
        "logoSize": "8rem",
        "outerGlow": "8rem",
        "innerGlow": "6rem",
        "pixels": 128
      },
      "xl": {
        "logoSize": "12rem",
        "outerGlow": "12rem",
        "innerGlow": "10rem",
        "pixels": 192
      },
      "central": {
        "logoSize": "25vw",
        "outerGlow": "25vw",
        "innerGlow": "25vw",
        "responsive": true
      }
    },
    "exportOptions": {
      "formats": ["PNG", "SVG", "WebP"],
      "resolutions": {
        "1K": { "width": 1024, "height": 1024 },
        "2K": { "width": 2048, "height": 2048 },
        "4K": { "width": 4096, "height": 4096 },
        "8K": { "width": 8192, "height": 8192 }
      },
      "transparency": true,
      "naming": "aegis-logo-{variant}-{resolution}.{format}"
    },
    "features": [
      "theme-aware",
      "animated-pulse",
      "responsive-sizing",
      "background-toggle", 
      "click-handler",
      "export-ready"
    ],
    "usage": {
      "automatic": "<AegisLogo size='md' enableAnimation={true} />",
      "specificVariant": "<AegisLogo variant='royal-dark' size='lg' showBackground={false} />",
      "clickable": "<AegisLogo size='central' onClick={() => navigateHome()} />"
    }
  }
